[flake8]
ignore =
    # Missing docstring
    D1
    # Missing blank line after last docstring section
    D413
exclude =
    .git,
    __pycache__,
    */migrations,
    venv
max-complexity = 10
max-line-length = 127

# docstrings
docstring-convention = google

# import order
import-order-style = pycharm
# application-import-names =
#     members
#     tutti
#     sync
